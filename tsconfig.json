{
    "compilerOptions": {
      "rootDir": "src",
      "outDir": "lib",
      "target": "ES6",
      "lib": ["ES6","DOM"],
      "module": "CommonJS",
      "moduleResolution": "node",
      "esModuleInterop": true,
      "noUnusedLocals": false,
      "noImplicitAny": true
    },
    "include": ["./src/**/*.tsx", "./src/**/*.ts", "src/Uploadfile.ts"],
    "exclude": [
      "lib",
      "node_modules",
      "**/__*__/**",
      "./src/**/*.spec.tsx",
      "./src/**/*.spec.ts"
    ]
  }